<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <description>Simple demo Flow that demonstrates how to use the LeadScorer invocable method. This Flow would need to be manually configured in the Flow Builder to work with the LeadScorer Apex class.</description>
    <interviewLabel>Lead Scoring Demo {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Lead Scoring Demo</label>
    <processMetadataValues>
        <name>BuilderType</name>
        <value>
            <stringValue>LightningFlowBuilder</stringValue>
        </value>
    </processMetadataValues>
    <processMetadataValues>
        <name>CanvasMode</name>
        <value>
            <stringValue>AUTO_LAYOUT_CANVAS</stringValue>
        </value>
    </processMetadataValues>
    <processType>Flow</processType>
    <start>
        <locationX>176</locationX>
        <locationY>0</locationY>
    </start>
</Flow>